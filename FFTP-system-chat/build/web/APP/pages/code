                                <span class="h5">All users</span>
                            </div>

                            <div>
                                <!-- JAVA SET all user -> number(size) -->
                                <span class="badge badge-danger">${fn:length(usersList)} members</span>
                            </div>

                        </div>

                        <div class="divider"></div>

                        <div class="widget-content table-responsive">

                            <table class="table">
                                <!-- COLUM -->
                                <thead>
                                    <tr>
                                        <th>
                                        </th>

                                        <th>
                                            Name
                                        </th>

                                        <th>
                                            Position
                                        </th>

                                        <th>
                                            E-Mail
                                        </th>

                                        <th>
                                            Phone
                                        </th>

                                    </tr>
                                </thead>

                                <tbody>
                                    <!-- JAVA INSERT (FOREACH) users ... -->
                                    <c:forEach items="${usersList}" var="_user">
                                    <tr>
                                        <td>
                                            <!-- JAVA SET PROFILE PICTURE -->
                                            <img class="avatar" src="../${_user.getProfilePicture()}">
                                        </td>

                                        <td>
                                            <!-- JAVA SET F, L name -->
                                            <span> ${_user.getFirstName()} ${_user.getLastName()} </span>
                                        </td>

                                        <td>
                                            <!-- JAVA SET TYPE WORK -->
                                            <span> ${_user.getTypeWork()} </span>
                                        </td>

                                        <td>
                                            <!-- JAVA SET EMAIL -->
                                            <span> ${_user.getEmail()} </span>
                                        </td>

                                        <td>
                                            <!-- JAVA SET PHONE NUMBER -->
                                            <span>${_user.getPhoneNumber()} </span>
                                        </td>
                                    </tr>
                                    </c:forEach>
                                </tbody>
                            </table>

                        </div>

                    </div>
                </div>
